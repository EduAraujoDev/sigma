{% extends "base_layout.twig" %}
{% block title %}Editar Ordem de Serviço{% endblock %}
{% block content_header %}
    <section class="content-header">
        <h1>
            Área Administrador
            <small>Editar Ordem de Serviço</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="{{base_url}}"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="{{base_url}}ordemservico/listar">Listar Ordem de Serviço</a></li>
            <li class="active">Editar Ordem de Serviço</li>
        </ol>
    </section>
{% endblock %}
{% block content %}
    <section class="content">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    {% if message_success is not empty %}
                        <div class="alert alert-success alert-dismissible fade in" role="alert" id="alerta">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">×</span>
                            </button>
                            <strong>Atenção!</strong>
                            {{message_success}}
                        </div>
                    {% endif %}                  
                    <h3 class="box-title">Dados do Ordem de Serviço</h3>
                </div>
                <form class="form-horizontal" role="form" method="post" action="{{base_url}}ordemservico/adicionar">
                    <div class="box-body">                      
                        <div class="form-group">
                            <div class="col-xs-12 col-sm-3 col-md-3" >
                                <label for="codOrdemServico">Código</label>
                                <input type="text" class="form-control" name="codOrdemServico" id="codOrdemServico" value="{{ ordemServico_cabecalho.id_ordem_servico }}" readonly>
                            </div>
                            <div class="col-xs-12 col-sm-3 col-md-3" >
                                <label for="codOrcamento">Orçamento</label>
                                <input type="text" class="form-control" name="codOrcamento" id="codOrcamento" value="{{ ordemServico_cabecalho.id_orcamento }}" readonly>
                            </div>                            
                            <div class="col-xs-12 col-sm-3 col-md-3">
                                <label for="statusOrdemServico">Status</label>
                                <select class="form-control" name="statusOrdemServico" id="statusOrdemServico">
                                    {% set statusOrdemServico = 0 %}
                                    {% for statu in status %}
                                        {% if statu.id_status == ordemServico_cabecalho.id_status %}
                                            {% set statusOrdemServico = statu.id_status %}
                                            <option selected="selected" value="{{ statu.id_status }}">{{ statu.titulo }}</option>
                                        {% else %}
                                            <option value="{{ statu.id_status }}">{{ statu.titulo }}</option>
                                        {% endif %}
                                    {% endfor %}
                                </select>
                            </div>
                            <div class="col-xs-12 col-sm-3 col-md-3">
                                <label for="tipoPagamento">Tipo de Pagamento</label>
                                <select class="form-control" name="tipoPagamento">
                                    {% for tipoPagamento in tipoPagamentos %}
                                        {% if tipoPagamento.id_tipo_pagamento == ordemServico_cabecalho.id_tipo_pagamento %}
                                            <option selected="selected" value="{{ tipoPagamento.id_tipo_pagamento }}">{{ tipoPagamento.titulo }}</option>
                                        {% else %}
                                            <option value="{{ tipoPagamento.id_tipo_pagamento }}">{{ tipoPagamento.titulo }}</option>
                                        {% endif %}
                                    {% endfor %}
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-12 col-sm-3 col-md-4" >
                                <label for="dataCriacao">Data Criação</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" name="dataCriacao" id="dataCriacao" value="{{ordemServico_cabecalho.data_criacao|date("d/m/Y")}}" readonly>
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                </div>
                            </div>                          
                            <div class="col-xs-12 col-sm-3 col-md-4" >
                                <label for="dataPrevistaFinalizacao">Previsão de finalização</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" id="dataPrevistaFinalizacao" name="dataPrevistaFinalizacao" value="{{ordemServico_cabecalho.data_prevista_finalizacao|date("d/m/Y")}}">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-3 col-md-4" >
                                <label for="dataFinalizacao">Data de finalização</label>
                                <div class="input-group">
                                    {% if ordemServico_cabecalho.data_finalizacao is null %}
                                        <input type="text" class="form-control" id="dataFinalizacao" name="dataFinalizacao" value="  /  /    " readonly>
                                    {% else %}
                                        <input type="text" class="form-control" id="dataFinalizacao" name="dataFinalizacao" value="{{ordemServico_cabecalho.data_finalizacao|date("d/m/Y")}}" readonly>
                                    {% endif %}                                    
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-12 col-sm-12 col-md-3" id="divNome">
                                <label for="codCliente">Código do Cliente</label>                            
                                <div class="input-group">
                                    <input type="text" class="form-control" name="codCliente" id="codCliente" placeholder="Código do Cliente" onBlur="verificarCodigo();" value="{{ ordemServico_cabecalho.id_cliente }}" readonly>
                                    <span class="input-group-btn">
                                        <button type="button" class="btn btn-info btn-flat" onclick="buscaCliente();"><i class="fa fa-search"></i></button>
                                    </span>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-9">
                                <label for="nomeCliente">Nome do Cliente</label>
                                {% for cliente in clientes %}
                                    {% if cliente.id_cliente == ordemServico_cabecalho.id_cliente %}
                                        <input type="text" class="form-control" name="nomeCliente" id="nomeCliente" placeholder="Nome do Cliente" value="{{ cliente.nome }}" readonly>
                                    {% endif %}
                                {% endfor %}
                            </div>  
                        </div>
                        <div class="form-group">
                            <div class="col-xs-12 col-sm-12 col-md-12">
                                <label for="observacoes">Obervações</label>
                                <br>
                                <textarea class="form-control" name="observacoes" rows="3" placeholder="" value="{{ ordemServico_cabecalho.observacoes }}"></textarea>
                            </div>  
                        </div>
                        <div class="form-group">
                            <div class="col-xs-12 col-sm-12 col-md-12">
                                <label for="servicos">Serviços</label>
                                <table id="servicos" class="table table-bordered table-hover table-striped">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th class="text-center">Código</th>
                                            <th class="text-center">Descrição</th>
                                            <th class="text-center">Valor Serviço</th>
                                            <th class="text-center">Valor Cobrado</th>
                                            <th class="text-center">Ação</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {% set quantServico = 0 %}
                                        {% set valorServico = 0 %}

                                        {% for ordemServico_servico in ordemServico_servicos %}
                                            {% set quantServico = quantServico + 1 %}

                                            <tr>
                                                <td><input type="hidden" id="servico_linha_{{ quantServico }}" name="servico_linha_{{ quantServico }}" value="{{ quantServico }}">{{ quantServico }}</td>
                                                <td><input type="hidden" id="servico_codigo_{{ quantServico }}" name="servico_codigo_{{ quantServico }}" value="{{ ordemServico_servico.id_servico }}">{{ ordemServico_servico.id_servico }}</td>                                                

                                                {% for servico in servicos %}
                                                    {% if ordemServico_servico.id_servico == servico.id_servico %}
                                                        <td><input type="hidden" id="servico_desc_{{ quantServico }}" name="servico_desc_{{ quantServico }}" value="{{ servico.descricao }}">{{ servico.descricao }}</td>
                                                        <td class="text-right"><input type="hidden" id="servico_vlrServico_{{ quantServico }}" name="servico_vlrServico_{{ quantServico }}" value="{{ servico.valor|number_format(2, ',', '.') }}">{{ servico.valor|number_format(2, ',', '.') }}</td>                                                        
                                                    {% endif %}
                                                {% endfor %}

                                                <td class="text-right"><input type="hidden" id="servico_vlrCobrado_{{ quantServico }}" name="servico_vlrCobrado_{{ quantServico }}" value="{{ ordemServico_servico.preco_cobrado|number_format(2, ',', '.') }}">{{ ordemServico_servico.preco_cobrado|number_format(2, ',', '.') }}</td>
                                                <td>
                                                    <button type="button" class="btn btn-info btn-flat" id="{{ quantServico }}"onclick="editarServico(this.id);"><i class="fa fa-edit"></i></button>&nbsp;
                                                    <button type="button" class="btn btn-danger btn-flat" id="{{ quantServico }}"onclick="pergRemoveServico(this.id);"><i class="fa fa-remove"></i></button>
                                                </td>                                                
                                            </tr>

                                            {% set valorServico = valorServico + ordemServico_servico.preco_cobrado %}
                                        {% endfor %}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3 col-md-offset-6"> 
                                <label for="totalServico" style="padding-top: 6px; padding-left: 50px">Total de Serviços(R$):</label>
                            </div>
                            <div class="col-md-3">
                                <input type="text" class="form-control text-right" name="totalServico" id="totalServico" value="{{valorServico|number_format(2, ',', '.')}}" readonly>
                            </div>
                        </div>
                        <hr>
                        <div class="form-group">
                            <div class="col-xs-12 col-sm-12 col-md-12">
                                <label for="produtos">Produtos</label>
                                <table id="produtos" class="table table-bordered table-hover table-striped">
                                    <thead>
                                        <tr class="titlerow">
                                            <th>#</th>
                                            <th class="text-center">Código</th>
                                            <th class="text-center">Descrição</th>
                                            <th class="text-center">Quantidade</th>
                                            <th class="text-center">Desconto</th>
                                            <th class="text-center">Preço Produto</th>
                                            <th class="text-center">Preço Cobrado</th>
                                            <th class="text-center">Subtotal Bruto</th>
                                            <th class="text-center">Subtotal Liquido</th>
                                            <th class="text-center">Ação</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {% set quantProduto = 0 %}
                                        {% set valorProduto = 0 %}
                                        {% set totalBrutoProduto = 0.00 %}
                                        {% set totalLiquidoProduto = 0.00 %}

                                        {% for ordemServico_produto in ordemServico_produtos %}
                                            {% set quantProduto = quantProduto + 1 %}
                                            <tr>
                                                <td><input type="hidden" id="produto_linha_{{ quantProduto }}" name="produto_linha_{{ quantProduto }}" value="{{ quantProduto }}">{{ quantProduto }}</td>
                                                <td><input type="hidden" id="produto_codigo_{{ quantProduto }}" name="produto_codigo_{{ quantProduto }}" value="{{ ordemServico_produto.id_produto }}">{{ ordemServico_produto.id_produto }}</td>                                                
                                  
                                                {% for produto in produtos %}
                                                    {% if ordemServico_produto.id_produto == produto.id_produto %}
                                                        <td><input type="hidden" id="produto_desc_{{ quantProduto }}" name="produto_desc_{{ quantProduto }}" value="{{ produto.nome }}">{{ produto.nome }}</td>
                                                    {% endif %}
                                                {% endfor %}

                                                <td class="text-right"><input type="hidden" id="produto_quantidade_{{ quantProduto }}" name="produto_quantidade_{{ quantProduto }}" value="{{ ordemServico_produto.quantidade }}">{{ ordemServico_produto.quantidade }}</td>
                                                <td class="text-right"><input type="hidden" id="produto_desconto_{{ quantProduto }}" name="produto_desconto_{{ quantProduto }}" value="{{ ordemServico_produto.desconto|number_format(0) }}">{{ ordemServico_produto.desconto|number_format(0) }}%</td>
                                                <td class="text-right"><input type="hidden" id="produto_prcProduto_{{ quantProduto }}" name="produto_prcProduto_{{ quantProduto }}" value="{{ ordemServico_produto.preco_venda|number_format(2, ',', '.') }}">{{ ordemServico_produto.preco_venda|number_format(2, ',', '.') }}</td>
                                                <td class="text-right"><input type="hidden" id="produto_prcCobrado_{{ quantProduto }}" name="produto_prcCobrado_{{ quantProduto }}" value="{{ ordemServico_produto.preco_cobrado|number_format(2, ',', '.') }}">{{ ordemServico_produto.preco_cobrado|number_format(2, ',', '.') }}</td>

                                                {% set totalBrutoProduto = ordemServico_produto.quantidade * ordemServico_produto.preco_cobrado %}
                                                <td class="text-right"><input type="hidden" id="produto_subTotalBr_{{ quantProduto }}" name="produto_subTotalBr_{{ quantProduto }}" value="{{ totalBrutoProduto|number_format(2, ',', '.') }}">{{ totalBrutoProduto|number_format(2, ',', '.') }}</td>

                                                {% set totalLiquidoProduto = totalBrutoProduto - (totalBrutoProduto * ordemServico_produto.desconto / 100) %}
                                                <td class="text-right"><input type="hidden" id="produto_subTotalQl_{{ quantProduto }}" name="produto_subTotalQl_{{ quantProduto }}" value="{{ totalLiquidoProduto|number_format(2, ',', '.') }}">{{ totalLiquidoProduto|number_format(2, ',', '.') }}</td>

                                                <td>
                                                    <button type="button" class="btn btn-info btn-flat" id="{{ quantProduto }}" onclick="editarProduto(this.id,{{ ordemServico_produto.id_produto }});"><i class="fa fa-edit"></i></button>&nbsp;
                                                    <button type="button" class="btn btn-danger btn-flat" id="{{ quantProduto }}" onclick="pergRemoveProduto(this.id);"><i class="fa fa-remove"></i></button>
                                                </td>                                                
                                            </tr>

                                            {% set quantProduto = quantProduto + 1 %}
                                            {% set valorProduto = valorProduto + totalLiquidoProduto %}
                                        {% endfor %}                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3 col-md-offset-6">
                                <label for="totalProduto" style="padding-top: 6px; padding-left: 50px">Total de Produtos(R$):</label>
                            </div>
                            <div class="col-md-3">
                                <input type="text" class="form-control text-right" name="totalProduto" id="totalProduto" value="{{valorProduto|number_format(2, ',', '.')}}" readonly>
                            </div>
                        </div>
                        <hr>
                        <div class="form-group">
                            <div class="col-md-6">
                            </div>
                            <div class="col-md-3">
                                <label for="totalValorBruto" style="padding-top: 6px; padding-left: 50px">Valor Total Bruto:</label>
                            </div>
                            <div class="col-md-3">
                                <input type="text" class="form-control text-right" name="totalValorBruto" id="totalValorBruto" value="{{ ordemServico_cabecalho.total_bruto|number_format(2, ',', '.') }}" readonly>
                            </div>
                        </div>
                        <hr>
                        <div class="form-group">
                            <div class="col-md-6">
                            </div>
                            <div class="col-md-3">
                                <label for="descontoAdicional" style="padding-top: 6px; padding-left: 50px">Desconto Adicional:</label>
                            </div>
                            <div class="col-md-3">
                                <div class="input-group">
                                    <input type="number" class="form-control" name="descontoAdicional" id="descontoAdicional" value="{{ ordemServico_cabecalho.desconto_adicional }}">
                                    <span class="input-group-addon">%</span>
                                </div>
                            </div>
                        </div>
                        <hr>
                        <div class="form-group">
                            <div class="col-md-6">                         
                            </div>
                            <div class="col-md-3">
                                <label for="totalValorLiquido" style="padding-top: 6px; padding-left: 50px">Valor Total Liquido:</label>
                            </div>
                            <div class="col-md-3">
                                <input type="text" class="form-control  text-right" name="totalValorLiquido" id="totalValorLiquido" value="{{ ordemServico_cabecalho.total_liquido|number_format(2, ',', '.') }}" readonly>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-6">
                            </div>
                            <div class="col-md-3">
                                <label for="descontoTotal" style="padding-top: 6px; padding-left: 50px">Desconto Total:</label>
                            </div>
                            <div class="col-md-3">
                                <div class="input-group">
                                    <input type="number" class="form-control" name="descontoTotal" id="descontoTotal" value="{{ ordemServico_cabecalho.desconto_total }}" readonly>
                                    <span class="input-group-addon">%</span>
                                </div>           
                            </div>
                        </div>
                    </div>
                    <div class="box-footer text-right">
                        {% if statusOrdemServico == 3 or statusOrdemServico == 4 %}
                            {% if ordemServico_cabecalho.data_finalizacao is null %}
                                <span id="spanBotaoFinalizar"><button type="button" id="botaoFinalizar" class="btn btn-success" onclick="alertMudStatus({{ statusOrdemServico }});">Finalizar</button></span>
                            {% else %}
                                <span id="spanBotaoFinalizar"></span>
                            {% endif %}
                            <span id="spanBotaoAlterar"></span>
                        {% elseif statusOrdemServico == 1 or statusOrdemServico == 2 %}
                            <span id="spanBotaoFinalizar"></span>
                            <span id="spanBotaoAlterar"><a class="btn btn-success" id="botaoAlterar" href="{{base_url}}ordemservico/editar/{{ ordemServico_cabecalho.id_orcamento }}">Alterar</a></span>
                        {% else %}
                            <span id="spanBotaoFinalizar"></span>
                            <span id="spanBotaoAlterar"></span>
                        {% endif %}
                        <a href="{{base_url}}ordemservico/listar"><button type="button" class="btn btn-danger">Cancelar</button></a>
                    </div>
                </form>
            </div>
        </div>
    </section>
    <div class="modal modal-info fade" id="modal_info" role="dialog">
        <div class="modal-dialog" style="padding-top: 15%">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <p class="modal-title-sec"></p>
                </div>
                <div class="modal-footer">
                    <div class="modalInfoBtn"></div>
                </div>
            </div>
        </div>
    </div>
   <!-- Modals -->
    <div class="modal fade" id="modal_cliente" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title">Escolha o cliente</h3>
                    <p>Clique duas vezes para escolher o cliente</p>
                </div>
                <div class="modal-body form">
                    <table id="tabelaCliente" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Nome</th>
                                <th>Cpf/Cnpj</th>
                                <th>E-Mail</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>            
                    </table>                    
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" onclick="fechaModalCli();">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modal_servico" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title">Escolha o Serviço</h3>
                    <p>Clique duas vezes para escolher o Serviço</p>
                </div>
                <div class="modal-body form">
                    <table id="tabelaServico" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Titulo</th>
                                <th>Descrição</th>
                                <th>Valor</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>            
                    </table>                    
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" onclick="fechaModalServ();">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modal_produto" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title">Escolha o Produto</h3>
                    <p>Clique duas vezes para escolher o Produto</p>
                </div>
                <div class="modal-body form">
                    <table id="tabelaProduto" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Descrição</th>
                                <th>Estoque</th>
                                <th>Reservado</th>
                                <th>Valor (R$)</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" onclick="fechaModalProd();">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modal_edit_servico" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">                    
                    <h3 class="modal-title">Serviço</h3>
                </div>
                <div class="modal-body form">
                    <form action="#" id="form" class="form-horizontal">
                        <input type="hidden" value="" name="linhaServico" id="linhaServico"/>
                        <div class="form-body">
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdServDesc">Descrição</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="mdServDesc" id="mdServDesc" disabled>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdServVlr">Valor Sugerido</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="mdServVlr" id="mdServVlr" disabled>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdServVlrCobr">Valor Cobrado</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="mdServVlrCobr" id="mdServVlrCobr">
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer text-right">
                    <button type="button" id="btnSave" onclick="salvaServico();" class="btn btn-success">Confirmar</button>
                    <button type="button" class="btn btn-default" onclick="fechaModalAltServ();">Cancelar</button>
                </div>
            </div>
        </div>        
    </div>
    <div class="modal fade" id="modal_edit_produto" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content box">
                <div class="modal-header">                    
                    <h3 class="modal-title">Produto</h3>
                </div>
                <div class="modal-body form">
                    <form action="#" id="form" class="form-horizontal">
                        <input type="hidden" value="" name="linhaProduto" id="linhaProduto"/>
                        <div class="form-body">
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdProdDesc">Descrição</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="mdProdDesc" id="mdProdDesc" disabled>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdProdVlr">Valor Sugerido</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="mdProdVlr" id="mdProdVlr" disabled>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdProdCobVlr">Valor Cobrado</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="mdProdCobVlr" id="mdProdCobVlr" onkeyup="calcProd();">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdProdQuant">Quantidade</label>
                                <div class="col-md-2">
                                    <input type="text" class="form-control" name="mdProdQuant" id="mdProdQuant" onkeyup="calcProd();" placeholder="0" value="0">
                                </div>

                                <label class="control-label col-md-4" for="mdProdQuantEst">Quantidade em estoque</label>
                                <div class="col-md-2">
                                    <input type="text" class="form-control" name="mdProdQuantEst" id="mdProdQuantEst" value="0" disabled>
                                </div>                                
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3" for="mdProdDs">Desconto</label>
                                <div class="col-md-9">
                                    <div class="input-group">
                                        <input type="number" class="form-control" name="mdProdDs" id="mdProdDs" onkeyup="calcProd();" placeholder="0" value="0">
                                        <span class="input-group-addon">%</span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-5 col-md-offset-1">
                                    <label class="control-label" for="mdProdTotBr">Subtotal Bruto</label>
                                    <input type="text" class="form-control" name="mdProdTotBr" id="mdProdTotBr" placeholder="0,00" disabled>
                                </div>
                                <div class="col-md-5">
                                    <label class="control-label" for="mdProdTotLq">Subtotal Liquido</label>
                                    <input type="text" class="form-control" name="mdProdTotLq" id="mdProdTotLq" placeholder="0,00" disabled>
                                </div>                                
                            </div>                              
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnSave" onclick="salvaProduto();" class="btn btn-success">Confirmar</button>
                    <button type="button" class="btn btn-default" onclick="fechaModalAltProd();">Cancelar</button>
                </div>
                <div id="overlayProduto">
                    <div class="overlay">
                        <i class="fa fa-refresh fa-spin"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>    
    <div class="modal modal-danger fade" id="modal_alerta" role="dialog">
        <div class="modal-dialog" style="padding-top: 15%">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title"></h3>
                </div>
                <div class="modal-body">
                    <h2 class="modal-title-sec"></h2>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" onclick="fechaModalAlerta();">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal modal-info fade" id="modal_info" role="dialog">
        <div class="modal-dialog" style="padding-top: 15%">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <p class="modal-title-sec"></p>
                </div>
                <div class="modal-footer">
                    <div class="modalInfoBtn"></div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
